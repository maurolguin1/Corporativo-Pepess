<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="pos_config_form_inherit_account_defaults_maxytul" model="ir.ui.view">
	    <field name="name">pos.config.form.inherit.account_defaults_maxytul</field>
	    <field name="model">pos.config</field>
	    <field name="inherit_id" ref="point_of_sale.pos_config_view_form"/>
	    <field name="arch" type="xml">
	    	<xpath expr="//div[@id='inventory_location']" position="inside">
	    		<div class="col-xs-12 col-md-6 o_setting_box" title="Warehouse from where this pos get served.">
                    <div class="o_setting_right_pane">
                        <label for="warehouse_id" string="Warehouse Served"/>
                        <div class="text-muted">
                            Warehouse from where this pos get served
                        </div>
                        <div class="content-group mt16">
                            <field name="warehouse_id"/>
                        </div>
                    </div>
                </div>
	    	</xpath>
	    </field>
	</record>
</odoo>